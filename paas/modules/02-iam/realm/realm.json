{
  "realm": "soul-fin",
  "displayName": "Soul Fin Realm",
  "enabled": true,
  "sslRequired": "none",
  "requiredActions": [],
  "registrationAllowed": false,
  "resetPasswordAllowed": true,
  "verifyEmail": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "editUsernameAllowed": false,
  "bruteForceProtected": true,
  "failureFactor": 5,
  "maxFailureWaitSeconds": 900,
  "minimumQuickLoginWaitSeconds": 60,
  "waitIncrementSeconds": 60,
  "quickLoginCheckMilliSeconds": 1000,
  "maxDeltaTimeSeconds": 43200,
  "permanentLockout": false,
  "accessTokenLifespan": 300,
  "accessTokenLifespanForImplicitFlow": 900,
  "ssoSessionIdleTimeout": 1800,
  "ssoSessionMaxLifespan": 36000,
  "offlineSessionIdleTimeout": 2592000,
  "refreshTokenMaxReuse": 0,
  "revokeRefreshToken": false,
  "users": [
    {
      "username": "user",
      "enabled": true,
      "requiredActions": [],
      "credentials": [
        {
          "type": "password",
          "value": "password",
          "temporary": false
        }
      ],
      "realmRoles": [
        "user",
        "account.read"
      ]
    },
    {
      "username": "super_user",
      "enabled": true,
      "requiredActions": [],
      "credentials": [
        {
          "type": "password",
          "value": "password",
          "temporary": false
        }
      ],
      "realmRoles": [
        "super_user",
        "account.read",
        "account.write"
      ]
    },
    {
      "username": "admin",
      "enabled": true,
      "requiredActions": [],
      "credentials": [
        {
          "type": "password",
          "value": "password",
          "temporary": false
        }
      ],
      "realmRoles": [
        "admin",
        "account.read",
        "account.write",
        "account.delete"
      ]
    },
    {
      "username": "external",
      "enabled": true,
      "requiredActions": [],
      "credentials": [
        {
          "type": "password",
          "value": "password",
          "temporary": false
        }
      ],
      "realmRoles": [
        "external"
      ]
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "user",
        "description": "Standard user role"
      },
      {
        "name": "super_user",
        "description": "Elevated user role"
      },
      {
        "name": "admin",
        "description": "Administrator role"
      },
      {
        "name": "external",
        "description": "External user role"
      },
      {
        "name": "account.read",
        "description": "Read access to account endpoints"
      },
      {
        "name": "account.write",
        "description": "Write access to account endpoints"
      },
      {
        "name": "account.delete",
        "description": "Delete access to account endpoints"
      }
    ]
  },
  "clients": [
    {
      "clientId": "account-service",
      "name": "Account Service Client",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "account-service-secret",
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": true,
      "implicitFlowEnabled": false,
      "standardFlowEnabled": true,
      "publicClient": false,
      "protocol": "openid-connect",
      "redirectUris": [
        "http://account-service:8080/*",
        "http://localhost:8090/*",
        "urn:ietf:wg:oauth:2.0:oob"
      ],
      "webOrigins": [],
      "protocolMappers": [
        {
          "name": "username",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-property-mapper",
          "config": {
            "user.attribute": "username",
            "claim.name": "preferred_username",
            "access.token.claim": "true",
            "id.token.claim": "true"
          }
        },
        {
          "name": "roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "config": {
            "claim.name": "roles",
            "access.token.claim": "true",
            "id.token.claim": "true"
          }
        }
      ]
    }
  ],
  "browserFlow": "browser",
  "registrationFlow": "registration",
  "directGrantFlow": "direct grant",
  "resetCredentialsFlow": "reset credentials",
  "clientAuthenticationFlow": "clients",
  "requiredCredentials": [
    "password"
  ],
  "otpPolicyType": "totp",
  "otpPolicyAlgorithm": "HmacSHA1",
  "otpPolicyDigits": 6,
  "otpPolicyInitialCounter": 0,
  "otpPolicyLookAheadWindow": 1,
  "otpPolicyPeriod": 30
}